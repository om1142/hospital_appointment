<%- include('./partials/header') %>



  <div id="wrapper">    

    <%- include('./partials/nav') %>
    
    <div class="container mb-5">
      <div id="carouselExampleAutoplaying" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-inner">
          <div class="carousel-item active"  data-interval="1s">
            <img src="https://source.unsplash.com/random/1199x400/?food" class="d-block w-100" alt="Food photo">
            <div class="carousel-caption d-none d-md-block ">
              <h5>Eating is a necessity, but cooking is an art.</h5>
            </div>
          </div>
          <div class="carousel-item"  data-interval="1s">
            <img src="https://source.unsplash.com/random/1200x399/?food" class="d-block w-100 " alt="Food photo">
            <div class="carousel-caption d-none d-md-block">
              <h5>Food is not just fuel, it's information. It talks to your DNA and tells it what to do.</h5>
  
            </div>
          </div>
          <div class="carousel-item" data-interval="1s">
            <img src="https://source.unsplash.com/random/1200x400/?food" class="d-block w-100 " alt="Food photo">
            <div class="carousel-caption d-none d-md-block">
              <h5>Food is love made edible.</h5>
              
            </div>
          </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleAutoplaying"
          data-bs-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleAutoplaying"
          data-bs-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="visually-hidden">Next</span>
        </button>
      </div>
    </div>

    <div class= "container">
         <p class="text py-4 mb-3 fs-3 text-center"> Let's start learing togather</p>          
    </div>

    <%- include('./partials/footer') %>
    